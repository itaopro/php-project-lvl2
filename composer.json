{
    "name": "hexlet/code",
            "bin": [
                "bin/gendiff"
        ],
        "autoload": {
        "files": [
            "src/Cli.php",
            "src/Differ.php",
            "src/Parsers.php",
            "src/Ast.php",
            "src/Formatters.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Plain.php",
            "src/Formatters/Json.php"
        ]
    },
    "authors": [
        {
            "name": "Nikolai Malchikov",
            "email": "malchikov.ne@gmail.com"
        }
    ],
        "description": "hexlet php-package",
        "license":  "MIT",
    "require": {
        "docopt/docopt": "^1.0",
        "lstrojny/functional-php": "^1.17",
        "wp-cli/php-cli-tools": "^0.11.13",
        "symfony/yaml": "^5.4"
    },
        "require-dev" : {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "3.*",
        "phpstan/phpstan": "^1.1",
        "phpstan/extension-installer": "^1.1"
        },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
